{
  "manifest_version": 3,
  "name": "Google Account Router",
  "version": "1.0.0",
  "description": "Googleサービスのデフォルトアカウントを自動的に切り替えます。",
  "permissions": ["webNavigation", "tabs", "storage"],
  "host_permissions": ["*://*.google.com/*", "*://*.youtube.com/*"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  }
}
